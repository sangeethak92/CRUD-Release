pipeline {
    agent any
    options { skipDefaultCheckout() }
      stages {
		
		stage('Build and Package') {
		when {
			  
			   
				   branch comparator: 'REGEXP', pattern: 'origin/Release.*|origin/RELEASE.*|origin/Release-*|origin/RELEASE-*'
			  
			}
				steps {
        
        echo "pipeline"
        
        }
     }
  }
 }
				
				  
