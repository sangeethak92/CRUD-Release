pipeline {
   agent any

   stages {
       
	    stage('Starting the Release for QA') {
			    steps {
			        
			      xlrCreateRelease releaseTitle: 'Release for $BUILD_TAG', serverCredentials: 'XLReleaseServer', startRelease: true, template: 'Test/QAReleaseTemplate', variables: [[propertyName: 'BUILD_NUMBER', propertyValue: '${BUILD_NUMBER}'], [propertyName: 'PR_NUMBER', propertyValue: '${PR_NUMBER}']]
			      
			    }
		} 
			
    }
}
